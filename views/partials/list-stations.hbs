{{#each stations}}
<div class="box box-link-hover-shadow">
  <h2 class="columns">
    <div class="column is-two-thirds">
      <a href="/station/{{_id}}" class="title">
        {{title}}
      </a>
    </div>
    <div class="column">
      <a href="/station/{{_id}}" class="button">
        {{>icons/open}}
      </a>
    </div>
    <div class="column">
      <a href="/dashboard/deletestation/{{_id}}" class=" button">
        {{>icons/delete}}
      </a>
    </div>
  </h2>
</div>
{{/each}}